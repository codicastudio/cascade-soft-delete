{
  "name": "codicastudio/cascade-soft-delete",
  "description": "A random Codica Studio package.",
  "keywords": [
      ""
  ],
  "homepage": "https://github.com/codicastudio/cascade-soft-delete",
  "license": "MIT",
  "authors": [
      {
          "name": "Michael J Brown",
          "email": "mbrown@codicastudio.com",
          "homepage": "https://codicastudio.com",
          "role": "Developer"
      }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "illuminate/database": "^8.0",
    "illuminate/events": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Dyrynda\\Database\\Support\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "extra": {
      "sort-packages": true
    }
  }
}
